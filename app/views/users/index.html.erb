<%= render 'userprofile', user: current_user, book: @book %>

<div class="col-md-8 offset-md-1">
   <h2>Users</h2>

   <table class="table table-hover table-inverse">
   <thead>
    <tr>
     <th>image</th>
     <th>name</th>
     <th></th>
     <th class="col-3"></th>
    </tr>
   </thead>
   <tbody>
             <% @users.each do |f| %>
          <tr>
           <td>
             <%= link_to user_path(f.id) do %>
             <%= image_tag f.get_profile_image %>
             <%end%>
           </td>
           <td>
             <%= f.name %>
            </td>
           <td>
              <%= link_to "Show", user_path(f.id) %>
              <%end%>
           </td>
          </tr>
         </tbody>

   </table>
   </div>
